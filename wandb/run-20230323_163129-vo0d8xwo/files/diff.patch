+        learning_rate: float = 3e-4,
+        gamma: float = 0.99,
+        tau: float = 0.005,
+        buffer_size: int = int(4e5),
+        net_arch: List = [256, 256],
+        dynamics_net_arch: List = [200, 200, 200, 200],
