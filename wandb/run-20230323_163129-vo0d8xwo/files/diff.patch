+                    wb.log({"dynamics/predictions": wb.Image(plot), "global_step": self.global_step})
+                    plot.close()
+
+            if terminated or truncated:
+                obs, _ = self.env.reset()
+                self.num_episodes += 1
+
+                if self.log and "episode" in info.keys():
