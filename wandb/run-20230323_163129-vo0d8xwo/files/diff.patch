+    # Mario
+    "nes-py",
+    "gym-super-mario-bros",
+    # minecart
+    "scipy >=1.7.3",
+    # mujoco
+    "imageio >=2.14.1",
+    "mujoco >=2.3.0",
+    # highway
+    "highway-env >= 1.8",
+    # box2d
+    "box2d-py ==2.3.5",
+    "pygame ==2.1.3.dev8",
